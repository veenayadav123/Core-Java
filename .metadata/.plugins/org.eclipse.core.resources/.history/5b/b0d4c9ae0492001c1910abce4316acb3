package javabasics;

public class PrimeNumber {

	public static void main(String[] args) {
		int number,i,count;
		System.out.println("1 to 100 prime number are : ");
		for ( number = 1; number <= 100; number++) {
			count = 0;
			for ( i = 2; i <= number / 2; i++) {
				if (number % i == 0) {
					count++;
					break;
				}
			}
			if (count == 0 && number != 1) {

				System.out.println(" ");

			}
		}
	}
}
