package Excercise_4;

public class SecdLargQ16 {
	
	public static void main(String[] args) {

		int a[] = { 99,25,36,5,8,75,95 };

		int y = 0;
		
		int x = 0;

		int b[] = a;

		

		for (int i = 0; i < a.length; i++) {

			if (b[i] > x) {

				y = x;

				x = b[i];

			} else if ( b[i] > y && b[i] > x ) {

				y = b[i];
				
			}

		}
		
		System.out.println(y);
	}

}
