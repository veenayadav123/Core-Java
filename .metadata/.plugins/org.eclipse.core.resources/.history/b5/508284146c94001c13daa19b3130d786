package Excercise_4;

public class SecdLargQ16 {
	
	public static void main(String[] args) {

		int a[] = { 100,25,36,5,8,75,95,524,22 };
		
        int y = 0;
		
		int x = 0;

		

		for (int i = 1; i < a.length; i++) {

			if (a[i] > x) {

				y = x;

				x = b[i];

			} else if ( a[i] > y && a[i] < x ) {

				y = a[i];
				
			}

		}
		
		System.out.println(y);
	}

}
